        <div class="col-md-2"></div>
        <div class="col-md-10">

            <div class="row">
                <div class="col-md-12">
                    <h1 id="title">Liste des timbres amphoriques</h1>
                    <br />
                </div>
            </div>

        {% for entity in entities %}
            <div class="col-md-3">
                <a href="{{ path('eponyme_show', { 'id': entity.id }) }}">{{ entity.nom }}</a>
            </div>
        {% endfor %}

    </div>
</div>
{% endblock %}
